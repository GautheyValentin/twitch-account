{
  "compilerOptions": {
    "target":"es6",
    "experimentalDecorators":true,
    "emitDecoratorMetadata":true,
    "module":"commonjs",
    "outDir":"./dist",
    "esModuleInterop":true,
    "forceConsistentCasingInFileNames":true,
    "strict":true,
    "skipLibCheck":true,
    "useUnknownInCatchVariables":false,
    "declaration": true,
    "declarationDir": "./dist/types"
  }
}
